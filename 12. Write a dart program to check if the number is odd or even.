//Write a dart program to check if the number is odd or even.

import 'dart:io';

void main()
{
  print('Enter the number:');
  
  var number = int.parse(stdin.readLineSync()!);
  
  if(number % 2 == 1)
  {
    print("Odd Number: $number");
  }
  else if(number % 2 == 0)
  {
    print("Even Number: $number");
  }
  
  else
  {
    print("Not Applicable!");
  }
}
