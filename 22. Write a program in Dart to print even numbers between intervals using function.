//Write a program in Dart to print even numbers between intervals using function.


void main() {
  evenNumbers(1, 10);
  evenNumbers(11, 20);
  evenNumbers(21, 30);
}

void evenNumbers(int num1, int num2)
{
  for(var i = num1; i<= num2; i++)
  {
    if (i % 2 == 0)
    {
      print(i);
    }
  }
}
